{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/Components/*"
      ],
      "@hooks/*": [
        "src/Hooks/*"
      ],
      "@screens/*": [
        "src/Screens/*"
      ],
      "@config/*": [
        "src/Config/*"
      ]
    },
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "extends": "expo/tsconfig.base"
}
